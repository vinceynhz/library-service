<?xml version="1.0" encoding="UTF-8"?>
<jaxb:bindings xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
               xmlns:xs="http://www.w3.org/2001/XMLSchema"
               jaxb:version="2.0">

  <jaxb:bindings schemaLocation="Commons.xsd">

    <jaxb:bindings node="//xs:complexType[@name='abstractBook']">
      <jaxb:class ref="app.tandv.services.model.common.AbstractBook"/>
    </jaxb:bindings>

    <jaxb:bindings node="//xs:simpleType[@name='formatTypeEnum']">
      <jaxb:typesafeEnumClass ref="app.tandv.services.model.common.FormatTypeEnum"/>
    </jaxb:bindings>

  </jaxb:bindings>

</jaxb:bindings>
