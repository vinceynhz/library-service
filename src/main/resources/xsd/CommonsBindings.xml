<?xml version="1.0" encoding="UTF-8"?>
<jaxb:bindings xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
               xmlns:xs="http://www.w3.org/2001/XMLSchema"
               xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
               jaxb:version="2.0">

  <jaxb:bindings schemaLocation="Commons.xsd">
    <jaxb:bindings node="//xs:element[@type='csd:formatType']">
      <xjc:javaType name="app.tandv.services.data.entity.BookFormat"
                    adapter="app.tandv.services.model.binding.BookFormatAdapter"/>
    </jaxb:bindings>
  </jaxb:bindings>
</jaxb:bindings>
